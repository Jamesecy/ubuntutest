= rails_admin_form_for @file_model, url: new_file_model_path, html: { multipart: true, class: "form-horizontal denser"} do |form|
  = form.generate action: :create

  .form-actions
    %button.btn.btn-primary{type: 'submit', name: '_save', :'data-disable-with' => t('admin.actions.new_file_model.submit')}
      %i.icon-ok
      = t('admin.actions.new_file_model.submit')
    %span.extra_buttons
      %button.btn{type: 'submit', name: '_continue', :'data-disable-with' => t('admin.form.cancel')}
        %i.icon-remove
        = t('admin.form.cancel')